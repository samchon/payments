{
  "name": "@samchon/payment-backend",
  "version": "9.0.0-dev.20241127",
  "description": "Payment Backend Server",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "rimraf prisma/migrations && node bin/test",
    "------------------------BUILDS------------------------": "",
    "build": "npm run build:sdk && npm run build:main && npm run build:test",
    "build:api": "rimraf packages/api/lib && nestia sdk && tsc -p packages/api/tsconfig.json && rollup -c packages/api/rollup.config.js",
    "build:main": "rimraf lib && tsc && rollup -c",
    "build:prisma": "prisma generate --schema=prisma/schema.prisma",
    "build:sdk": "rimraf src/api/functional && nestia sdk",
    "build:swagger": "nestia swagger",
    "build:test": "rimraf bin && tsc -p test/tsconfig.json",
    "dev": "npm run build:test -- --watch",
    "eslint": "eslint src && eslint test",
    "eslint:fix": "eslint src --fix && eslint test --fix",
    "prepare": "ts-patch install && typia patch && npm run build:prisma",
    "------------------------WEBPACK------------------------": "",
    "webpack": "rimraf dist && npm run build:prisma && webpack",
    "webpack:start": "cd dist && node server",
    "webpack:test": "npm run webpack && rimraf prisma/migrations && node bin/test/webpack.js",
    "------------------------DEPLOYS------------------------": "",
    "schema": "node lib/executable/schema",
    "start": "node lib/executable/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/payments"
  },
